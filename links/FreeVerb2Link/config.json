{
    "label": "FreeVerb (stereo)",
    "class": "FreeVerb2Link",
    "options": [
        {
            "src": "mix",
            "label": "Dry/wet Balance",
            "type": "number",
            "defaultVal": 0.25,
            "field": {
                "type": "slider",
                "fieldOptions": {
                    "marks": [
                        {
                            "value": 0.02,
                            "label": "Dryer"
                        },
                        {
                            "value": 0.5,
                            "label": "Wetter"
                        }
                    ],
                    "logarithmic": true,
                    "min": 0,
                    "max": 1,
                    "step": 0.01,
                    "scale": 100
                }
            }
        },
        {
            "src": "room",
            "label": "Room Size",
            "type": "number",
            "defaultVal": 0.15,
            "field": {
                "type": "slider",
                "fieldOptions": {
                    "marks": [
                        {
                            "value": 0.2,
                            "label": "Smaller"
                        },
                        {
                            "value": 0.8,
                            "label": "Larger"
                        }
                    ],
                    "logarithmic": false,
                    "min": 0,
                    "max": 1,
                    "step": 0.01,
                    "scale": 100
                }
            }
        },
        {
            "src": "damp",
            "label": "Damping",
            "type": "number",
            "defaultVal": 0.5,
            "field": {
                "type": "slider",
                "fieldOptions": {
                    "marks": [
                        {
                            "value": 0.2,
                            "label": "Less"
                        },
                        {
                            "value": 0.8,
                            "label": "More"
                        }
                    ],
                    "logarithmic": false,
                    "min": 0,
                    "max": 1,
                    "step": 0.01,
                    "scale": 100
                }
            }
        }
   ]
}